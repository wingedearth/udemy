{
	"extends": "eslint:recommended",
	"env": {
		"es6": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "off",
		"no-console": "off"
	},
	"globals": {
		"React": false,
		"ReactDOM": false
	}
}